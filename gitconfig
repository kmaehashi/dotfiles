[core]
  excludesfile = ~/.gitignore
  quotepath = false
[push]
  default = tracking
[user]
  name = Kenichi Maehashi
  email = webmaster@kenichimaehashi.com
[color]
  ui = auto
[alias]
  gr = log --graph --date-order --pretty=format:'<%h> %ad %Cgreen%d%Creset %s  %Cblue[%an]%Creset' --all --date=short
  dc = diff --cached
  diffc = diff --cached
  co = checkout
  com = commit
  cp = cherry-pick
  br = branch
  st = status
  stat = status
  ru = remote update
  pr = pull-request
  wip = commit -m 'wip'
[help]
  autocorrect = -1
[diff]
  algorithm = histogram
