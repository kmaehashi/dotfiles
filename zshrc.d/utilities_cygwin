function c() {
  PATH="$(echo "${PATH}" | tr ':' '\n' | grep '^/cygdrive/' | tr '\n' ':')" cmd.exe /C "$@" 2>&1 | /bin/iconv -f "${WINDOWS_CHARSET}" -t utf-8 | /bin/sed 's/\n/\r\n/g'
}

function killall() {
  if [ -z "${1}" ]; then return; fi
  for EXT in ".exe" ".com"; do taskkill /f /im "${1}${EXT}" && break; done
}
